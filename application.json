[{
  "name": "hoist-webhooks-listener",
  "script": "./lib/server.js",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "/var/log/node/hoist-webhooks-listener.stderr.log",
  "out_file": "/var/log/node/hoist-webhooks-listener.stdout.log",
  "pid_file": "./pids/hoist-webhooks-listener.pid",
  "instances": "max",
  "cron_restart": "01 00 * * 0",
  "merge_logs": true,
  "exec_mode": "cluster_mode",
  "env": {
    "NODE_ENV": "production"
  }
}]
